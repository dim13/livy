# Floating point formats.

# %f and %F

)format "%f"
1 1e25 4/3
	1.000000 10000000000000000000000000.000000 1.333333

)format "%F"
-1 -1e25 -4/3
	-1.000000 -10000000000000000000000000.000000 -1.333333

)format "%.2f"
1 1e25 4/3
	1.00 10000000000000000000000000.00 1.33

)format "%.0f"
1 1e25 4/3
	1 10000000000000000000000000 1

# %e and %E

)format "%.1e"
1 100 1000 1000000 -123456
	1.0e+00 1.0e+02 1.0e+03 1.0e+06 -1.2e+05

)format "%.1E"
1 100 1000 1000000 -123456
	1.0E+00 1.0E+02 1.0E+03 1.0E+06 -1.2E+05

)format "%.9e"
1 100 1000 1000000 -123456
	1.000000000e+00 1.000000000e+02 1.000000000e+03 1.000000000e+06 -1.234560000e+05

)format "%.0e"
1 100 1000 1000000 -123456
	1e+00 1e+02 1e+03 1e+06 -1e+05

)format "%.1e"
123456e100 -123456e100
	1.2e+105 -1.2e+105

)format "%.9e"
123456e100 -123456e100
	1.234560000e+105 -1.234560000e+105

)format "%.0e"
123456e100 -123456e100
	1e+105 -1e+105

# Rationals with %e.

)format "%.1e"
1/3 -1/3 1/3000 10000/3
	3.3e-01 -3.3e-01 3.3e-04 3.3e+03

)format "%.0e"
1/100 1/3 -1/3 1/3000 10000/3
	1e-02 3e-01 -3e-01 3e-04 3e+03

)format "%.3e"
1/100 1/3 -1/3 1/3000 10000/3
	1.000e-02 3.333e-01 -3.333e-01 3.333e-04 3.333e+03

)format "%.9e"
1/3 -1/3 1/3000 10000/3
	3.333333333e-01 -3.333333333e-01 3.333333333e-04 3.333333333e+03


# %g and %G

)format "%.1g"
1 12 -123 1234 12345 123456 1234567 12345678
	1 1e+01 -1e+02 1e+03 1e+04 1e+05 1e+06 1e+07

)format "%.3g"
1 12 -123 1234 12345 123456 1234567 12345678
	1 12 -123 1.23e+03 1.23e+04 1.23e+05 1.23e+06 1.23e+07

)format "%.6G"
# TODO: rounding is wrong
1 12 -123 1234 12345 123456 1234567 12345678
	1 12 -123 1234 12345 123456 1.23456E+06 1.23456E+07

)format "%.9g"
1 12 -123 1234 12345 123456 1234567 12345678
	1 12 -123 1234 12345 123456 1234567 12345678

)format "%.1g"
1e20 1.2e20 -1.23e20 1.234e20 1.2345e20 1.23456e20 1.234567e20 1.2345678e20
	1e+20 1e+20 -1e+20 1e+20 1e+20 1e+20 1e+20 1e+20

)format "%.3g"
1e20 1.2e20 -1.23e20 1.234e20 1.2345e20 1.23456e20 1.234567e20 1.2345678e20
	1e+20 1.2e+20 -1.23e+20 1.23e+20 1.23e+20 1.23e+20 1.23e+20 1.23e+20


)format "%.6g"
# TODO: rounding is wrong
1e20 -1.2e20 1.23e20 1.234e20 1.2345e20 1.23456e20 1.234567e20 1.2345678e20
	1e+20 -1.2e+20 1.23e+20 1.234e+20 1.2345e+20 1.23456e+20 1.23456e+20 1.23456e+20

)format "%.9g"
1e20 -1.2e20 1.23e20 1.234e20 1.2345e20 1.23456e20 1.234567e20 1.2345678e20
	1e+20 -1.2e+20 1.23e+20 1.234e+20 1.2345e+20 1.23456e+20 1.234567e+20 1.2345678e+20

)format "%.20g"
12345678901234567890 -123456789012345678901 1234567890123456789012
	12345678901234567890 -1.234567890123456789e+20 1.234567890123456789e+21

)format "%.1g"
1/3 1/30 -1/300 1/3000 1/30000
	0.3 0.03 -0.003 0.0003 3e-05

)format "%.1G"
4/3 40/3 -400/3 4000/3
	1 1E+01 -1E+02 1E+03

)format "%.6g"
1/3 1/30 -1/300 1/3000 1/30000
	0.333333 0.0333333 -0.00333333 0.000333333 3.33333e-05

)format "%.6g"
4/3 40/3 -400/3 4000/3
	1.33333 13.3333 -133.333 1333.33

)format "%.9g"
1/3 1/30 -1/300 1/3000 1/30000
	0.333333333 0.0333333333 -0.00333333333 0.000333333333 3.33333333e-05

)format "%.9g"
4/3 40/3 -400/3 4000/3
	1.33333333 13.3333333 -133.333333 1333.33333

# Trailing zeros and decimals (%g/%G only)

# Fractional form.
)format "%.20g"
1 2 3 1/2 1/4 1/8 1e10
	1 2 3 0.5 0.25 0.125 10000000000

# Exponential form
)format "%g"
1e10
	1e+10

# Verify that )specials always use base 10.
)base 3
)base 10
1<<9
	512

# Big integers print as float. Configurable by )maxdigits.
)maxdigits 10
1e100
	1e+100

